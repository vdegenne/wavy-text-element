{
	"name": "wavy-text-element",
	"version": "0.1.4",
	"description": "A wavy text custom element.",
	"type": "module",
	"exports": {
		"import": "./lib/index.js",
		"types": "./lib/index.d.ts"
	},
	"files": [
		"./lib"
	],
	"scripts": {
		"build": "wireit"
	},
	"devDependencies": {
		"lit": "^3.3.1",
		"typescript": "^5.9.3",
		"wireit": "^0.14.12"
	},
	"peerDependencies": {
		"lit": "^3.0.0"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"./tsconfig.json",
				"./src"
			],
			"output": [
				"./lib"
			]
		}
	}
